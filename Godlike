game:GetService("ReplicatedStorage").rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", 2)
end)
if not success then
warn("Failed to send size change request: " .. tostring(errorMessage))
end
task.wait(0)
end
end)
else
if autoSizeLoop then
task.cancel(autoSizeLoop)
autoSizeLoop = nil
end
end
end)

