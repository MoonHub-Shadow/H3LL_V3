local AutoRebirth = window:AddTab("Auto Rebirth")
local folder11 = AutoRebirth:AddFolder("Basic Auto Rebirth")

folder11:AddTextBox("Target Input", function(Text)
local n = tonumber(Text)
if n and n >= 1 then
_targetRebirthTarget = n
else
-- handle invalid input if needed
end
end)

folder11:AddSwitch("Auto Rebirth Target", function(state)
_autorebirthtarget = state
if _autorebirthtarget then
    _autorebirthActive = true
    task.spawn(function()
        local leaderstats = LocalPlayer:WaitForChild("leaderstats")
        local rebirths = leaderstats and leaderstats:WaitForChild("Rebirths")
        local remote = game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("rebirthRemote")
        if not (rebirths and remote) then
            _autorebirthActive = false
            return
        end
        while _autorebirthActive do
            if rebirths.Value >= _targetRebirthTarget then
                _autorebirthActive = false
                print("Target rebirths reached! Stopping auto rebirth.")
                break
            end
            local success, errorMessage = pcall(function()
                remote:InvokeServer("rebirthRequest")
            end)
            if not success then
                warn("Failed to send rebirth request: " .. tostring(errorMessage))
            end
            task.wait(1)
        end
    end)
end
end)

folder11:AddSwitch("Auto Rebirth Infinite", function(State)
_autorebirthinfinite = State
if _autorebirthinfinite then
task.spawn(function()
while true do
local success, errorMessage = pcall(function()
game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("rebirthRemote"):InvokeServer("rebirthRequest")
end)
if not success then
warn("Failed to send rebirth request: " .. tostring(errorMessage))
end
task.wait(0.1)
end
end)
end
end)

folder11:AddSwitch("Lock Position", function(Value)
lockposition = Value
if lockposition then
local currentPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
getgenv().posLock = game:GetService("RunService").Heartbeat:Connect(function()
if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = currentPos
end
end)
else
if getgenv().posLock then
getgenv().posLock:Disconnect()
end
end
end)

